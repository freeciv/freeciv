#: data/experimental/units.ruleset:427 data/experimental/units.ruleset:471
msgid "?worker_level:apprentice"
msgstr "ученик"

#. TRANS: Worker veteran level
#: data/experimental/units.ruleset:429 data/experimental/units.ruleset:473
msgid "?worker_level:journeyman"
msgstr "подмастерье"

#. TRANS: Worker veteran level
#: data/experimental/units.ruleset:431 data/experimental/units.ruleset:475
