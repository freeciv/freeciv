#~ "уничтожены:"

#~ msgid ""
#~ " - farmland\n"
#~ " - irrigation\n"
#~ " - mining\n"
#~ " - a base (fortress, airbase, ...)\n"
#~ " - a railroad\n"
#~ " - a road"
#~ msgstr ""
#~ " - ферма\n"
#~ " - орошение\n"
#~ " - шахта\n"
#~ " - базы (крепость, авиабаза, ...)\n"
