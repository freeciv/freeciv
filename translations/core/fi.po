#: common/reqtext.c:883
#, c-format
msgid "?terrain:Requires %s on the tile."
msgstr "Edellyttää ruudun olevan maastotyyppiä %s."

#: common/reqtext.c:886
#, c-format
msgid "?terrain:Prevented by %s on the tile."
msgstr "Estyy jos ruutu on maastotyyppiä %s."

#: common/reqtext.c:894
#, c-format
msgid "?terrain:Requires %s on the tile or a cardinally adjacent tile."
msgstr ""
"Edellyttää ruudun tai sen rinnalla olevan ruudun olevan maastotyyppiä %s."

#: common/reqtext.c:899
#, c-format
msgid "?terrain:Prevented by %s on the tile or any cardinally adjacent tile."
msgstr "Estyy jos ruutu tai sen rinnalla oleva ruutu on maastotyyppiä %s."

#: common/reqtext.c:908
#, c-format
msgid "?terrain:Requires %s on the tile or an adjacent tile."
msgstr "Edellyttää ruudun tai viereisen ruudun olevan maastotyyppiä %s."

#: common/reqtext.c:913
#, c-format
msgid "?terrain:Prevented by %s on the tile or any adjacent tile."
