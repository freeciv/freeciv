#: ai/default/daicity.c:999
#, c-format
msgid "%s is selling %s for %d."
msgid_plural "%s is selling %s for %d."
msgstr[0] "%s realisoi omaisuuttaan: %s myyty hintaan %d."
msgstr[1] "%s realisoi omaisuuttaan: %s myyty hintaan %d."

# Away-moodissa pelaaja menee AI-hallinnoitavaksi.
#: ai/default/daidiplomacy.c:371
#, c-format
msgid "*%s (AI)* In away mode AI can't sign such a treaty."
msgstr ""
"*%s (AI)* AI ei voi allekirjoittaa tällaista sopimusta poissa olevan "
"pelaajan puolesta."

# AI teitittelee...
#: ai/default/daidiplomacy.c:381
#, c-format
msgid "*%s (AI)* First break alliance with %s, %s."
msgstr "*%s (AI)* Purkakaa ensin liittonne pelaajan %s kanssa, %s."

#: ai/default/daidiplomacy.c:391
#, c-format
msgid ""
"*%s (AI)* I would like to see you keep your distance from %s for some time, "
"%s."
msgstr ""
"*%s (AI)* Haluaisin ensin odottaa varmistuakseni ettette te ja %s enää ole "
"yhteistyössä, %s."

#: ai/default/daidiplomacy.c:405
#, c-format
msgid "*%s (AI)* Let us first cease hostilities, %s."
msgstr "*%s (AI)* Lopettakaamme ensin vihollisuutemme, %s."

#: ai/default/daidiplomacy.c:410
#, c-format
msgid ""
"*%s (AI)* I wish to see you keep the current ceasefire for a bit longer "
"first, %s."
msgstr ""
"*%s (AI)* Olisi suotavaa, jos ensin pitäisitte tulitauon hieman pidempään, "
"%s."

#: ai/default/daidiplomacy.c:433
#, c-format
msgid "*%s (AI)* I simply do not trust you with an alliance yet, %s."
msgstr "*%s (AI)* En luota teihin vielä riittävästi liittoutumista varten, %s."

#: ai/default/daidiplomacy.c:527
#, c-format
msgid "*%s (AI)* I do not know the city you mention."
msgstr "*%s (AI)* En tunne mainitsemaanne kaupunkia."

#: ai/default/daidiplomacy.c:558
#, c-format
msgid "*%s (AI)* Sorry, sharing vision with you is not safe."
msgstr "*%s (AI)* Pahoittelen, näkymän jako kanssanne ei ole turvallista."

#: ai/default/daidiplomacy.c:688
#, c-format
msgid "*%s (AI)* This deal was not very good for us, %s!"
msgstr "*%s (AI)* Tämä sopimus ei ollut erityisen hyvä kannaltamme, %s!"

#: ai/default/daidiplomacy.c:711
#, c-format
msgid "*%s (AI)* Welcome into our alliance %s!"
msgstr "*%s (AI)* Tervetuloa liittolaiseksemme, %s!"

#: ai/default/daidiplomacy.c:715
#, c-format
msgid "*%s (AI)* Yes, may we forever stand united, %s."
msgstr "*%s (AI)* Aivan, kestäköön liittomme ikuisesti, %s."

#: ai/default/daidiplomacy.c:722
#, c-format
msgid "*%s (AI)* Yes, peace in our time!"
msgstr "*%s (AI)* Aivan, rauha meidän aikanamme!"

#: ai/default/daidiplomacy.c:727
#, c-format
msgid "*%s (AI)* Agreed. No more hostilities, %s."
msgstr "*%s (AI)* Olen samaa mieltä. Lopettakaamme vihanpito, %s."

#: ai/default/daidiplomacy.c:994
#, c-format
msgid ""
"*%s (AI)* Greetings %s! May we suggest a ceasefire while we get to know each "
"other better?"
msgstr ""
"*%s (AI)* Tervehdys, %s! Voimmeko ehdottaa tulitaukoa jotta ehdimme paremmin "
"tutustua?"

#: ai/default/daidiplomacy.c:1002
#, c-format
msgid ""
"*%s (AI)* I found you %s! Now make it worth my letting you live, or be "
"crushed."
msgstr ""
"%s (AI)* Löysin sinut %s! Anna minulle hyvä syy jättää teidät murskaamatta "
"tai valmistaudu tuhoosi."

#: ai/default/daidiplomacy.c:1356
#, c-format
msgid "*%s (AI)* Space will never be yours."
msgstr "*%s (AI)* Avaruus ei tule koskaan kuulumaan teille."

#: ai/default/daidiplomacy.c:1362
#, c-format
msgid "*%s (AI)* I have tolerated your vicious antics long enough! To war!"
msgstr "*%s (AI)* Olen sietänyt äksyilyänne riittävän pitkään! Tulkoon sota!"

#: ai/default/daidiplomacy.c:1368
#, c-format
msgid "*%s (AI)* Peace in ... some other time."
msgstr "*%s (AI)* Rauhaa? Ehkä joskus toiste."

#: ai/default/daidiplomacy.c:1374
#, c-format
msgid ""
"*%s (AI)* Finally I get around to you! Did you really think you could get "
"away with your crimes?"
msgstr ""
"*%s (AI)* Lopultakin yhytin teidät! Luulitteko tosiaan pääsevänne kuin koira "
"veräjästä?"

#: ai/default/daidiplomacy.c:1381
#, c-format
msgid "*%s (AI)* Your covert hostilities brought this war upon you!"
msgstr "*%s (AI)* Desanttienne sabotaašitoiminta sai ansaitsemansa lopun!"

#: ai/default/daidiplomacy.c:1389
#, c-format
msgid "*%s (AI)* Your aggression against %s was your last mistake!"
msgstr "*%s (AI)* Liittolaiseni (%s) ahdistelu oli vihonviimeinen erheenne!"

#: ai/default/daidiplomacy.c:1496
#, c-format
msgid ""
"*%s (AI)* We will be launching an all-out war against %s in %d turn to stop "
"the spaceship launch."
msgid_plural ""
"*%s (AI)* We will be launching an all-out war against %s in %d turns to stop "
"the spaceship launch."
msgstr[0] ""
"*%s (AI)* %s saa tuta sotavoimiemme täyden mahdin %d vuoron kuluttua, jotta "
"avaruusaluksen laukaisu tulee estetyksi."
msgstr[1] ""
"*%s (AI)* %s saa tuta sotavoimiemme täyden mahdin %d vuoron kuluttua, jotta "
"avaruusaluksen laukaisu tulee estetyksi."

#: ai/default/daidiplomacy.c:1507
#, c-format
msgid ""
"*%s (AI)* Your aid in this matter will be expected. Long live our glorious "
"alliance!"
msgstr ""
"*%s (AI)* Odotamme teidän auttavan tässä asiassa. Kauan eläköön mahtava "
"liittomme!"

#: ai/default/daidiplomacy.c:1514
#, c-format
msgid ""
"*%s (AI)* %s has grossly violated their treaties with us for own gain.  We "
"will answer in force in %d turn and expect you to honor your alliance with "
"us and do likewise!"
msgid_plural ""
"*%s (AI)* %s has grossly violated their treaties with us for own gain.  We "
"will answer in force in %d turns and expect you to honor your alliance with "
"us and do likewise!"
msgstr[0] ""
"*%s (AI)* %s on vakavasti rikkonut kansakuntiemme välisiä sopimuksia omaksi "
"edukseen. Vastaamme voimakeinoin %d vuoron kuluttua ja odotamme teidän "
"kunnioittavan liittoumaamme toimimalla samoin!"
msgstr[1] ""
"*%s (AI)* %s on vakavasti rikkonut kansakuntiemme välisiä sopimuksia omaksi "
"edukseen. Vastaamme voimakeinoin %d vuoron kuluttua ja odotamme teidän "
"kunnioittavan liittoumaamme toimimalla samoin!"

#: ai/default/daidiplomacy.c:1528
#, c-format
msgid ""
"*%s (AI)* We intend to pillage and plunder the rich civilization of %s. We "
"declare war in %d turn."
msgid_plural ""
"*%s (AI)* We intend to pillage and plunder the rich civilization of %s. We "
"declare war in %d turns."
msgstr[0] ""
"*%s (AI)* %s ui rahassa, joten aiomme ryöstää ja rosvota hänen "
"kansakuntansa! Sota julistetaan %d vuoron kuluttua."
msgstr[1] ""
"*%s (AI)* %s ui rahassa, joten aiomme ryöstää ja rosvota hänen "
"kansakuntansa! Sota julistetaan %d vuoron kuluttua."

#: ai/default/daidiplomacy.c:1537
#, c-format
msgid ""
"*%s (AI)* If you want a piece of the loot, feel free to join in the action!"
msgstr "*%s (AI)* Jos haluatte osille ryöstösaaliista, tulkaa mukaan vaan!"

#: ai/default/daidiplomacy.c:1543
#, c-format
msgid ""
"*%s (AI)* We have had it with %s. Let us tear this pathetic civilization "
"apart. We declare war in %d turn."
msgid_plural ""
"*%s (AI)* We have had it with %s. Let us tear this pathetic civilization "
"apart. We declare war in %d turns."
msgstr[0] ""
"*%s (AI)* %s käy hermoillemme! Julistamme sodan %d vuoron kuluttua ja "
"revimme hänen naurettavan kansakuntansa silpuksi."
msgstr[1] ""
"*%s (AI)* %s käy hermoillemme! Julistamme sodan %d vuoron kuluttua ja "
"revimme hänen naurettavan kansakuntansa silpuksi."

#: ai/default/daidiplomacy.c:1554
#, c-format
msgid "*%s (AI)* As our glorious allies, we expect your help in this war."
msgstr ""
"*%s (AI)* Odotamme apuanne tässä sodassa; olettehan mahtavia liittolaisiamme."

#: ai/default/daidiplomacy.c:1561
#, c-format
msgid ""
"*%s (AI)* We will honor our alliance and declare war on %s in %d turn.  Hold "
"on - we are coming!"
msgid_plural ""
"*%s (AI)* We will honor our alliance and declare war on %s in %d turns.  "
"Hold on - we are coming!"
msgstr[0] ""
"*%s (AI)* Kunnioitamme liittoamme, alas %s! Julistamme sodan %d vuoron "
"kuluttua. Tulossa ollaan!"
msgstr[1] ""
"*%s (AI)* Kunnioitamme liittoamme, alas %s! Julistamme sodan %d vuoron "
"kuluttua. Tulossa ollaan!"

#: ai/default/daidiplomacy.c:1571
#, c-format
msgid ""
"*%s (AI)* We will honor our alliance with %s and declare war on %s in %d "
"turns.  We expect you to do likewise."
msgid_plural ""
"*%s (AI)* We will honor our alliance with %s and declare war on %s in %d "
"turns.  We expect you to do likewise."
msgstr[0] ""
"*%s (AI)* Koska liittolaisemme %s aikoo sotaan, on %s meidänkin "
"vihollisemme. Julistamme sodan %d vuoron kuluttua! Odotamme teidän toimivan "
"samoin."
msgstr[1] ""
"*%s (AI)* Koska liittolaisemme %s aikoo sotaan, on %s meidänkin "
"vihollisemme. Julistamme sodan %d vuoron kuluttua! Odotamme teidän toimivan "
"samoin."

#: ai/default/daidiplomacy.c:1652
#, c-format
msgid ""
"*%s (AI)* Your attempt to conquer space for yourself alone betrays your true "
"intentions, and I will have no more of our alliance!"
msgstr ""
"*%s (AI)* Yrityksesi valloittaa avaruus yksin itsellesi toi julki todelliset "
"aikomuksesi. Liittomme on tästä pitäen ohi!"

#: ai/default/daidiplomacy.c:1668
#, c-format
msgid ""
"*%s (AI)* Your attempt to unilaterally dominate outer space is highly "
"offensive."
msgstr ""
"*%s (AI)* Pyrkimyksesi hallita ulkoavaruutta yksin loukkaa meitä syvästi."

#: ai/default/daidiplomacy.c:1672
#, c-format
msgid ""
"*%s (AI)* If you do not stop constructing your spaceship, I may be forced to "
"take action!"
msgstr ""
"*%s (AI)* Jos et lopeta avaruusaluksesi rakentamista, minun pitää varmaankin "
"tehdä asialle jotakin!"

#: ai/default/daidiplomacy.c:1795
#, c-format
msgid "*%s (AI)* Sorry, sharing vision with you is no longer safe."
msgstr ""
"*%s (AI)* Pahoittelen, mutta näkymän jakaminen kanssasi ei ole enää "
"turvallista."

#: ai/default/daidiplomacy.c:1866
#, c-format
msgid ""
"*%s (AI)* Greetings our most trustworthy ally. We call upon you to destroy "
"our enemy, %s."
msgstr ""
"*%s (AI)* Tervehdys, oi luotettu liittolaisemme. Pyydämme apuanne - "
"vihollisemme %s pitää tuhota."

#: ai/default/daidiplomacy.c:1874
#, c-format
msgid ""
"*%s (AI)* Greetings ally, I see you have not yet made war with our enemy, "
"%s. Why do I need to remind you of your promises?"
msgstr ""
"*%s (AI)* Tervehdys, liittolainen. Huomaan, että vihollisemme, %s, ei "
"vieläkään ole vastaanottanut sodanjulistustasi. Pitääkö minun muistuttaa "
"teitä tekemistänne lupauksista?"

#: ai/default/daidiplomacy.c:1884
#, c-format
msgid ""
"*%s (AI)* Dishonored one, we made a pact of alliance, and yet you remain at "
"peace with our mortal enemy, %s! This is unacceptable; our alliance is no "
"more!"
msgstr ""
"*%s (AI)* Oi kunniaton, liittomme vastaisesti verivihollisemme, %s, saa "
"vieläkin juonia rauhassa, ettekä nosta sormeannekaan! Tällaista on mahdoton "
"hyväksyä, liittomme olkoon purettu!"

#: ai/default/daidiplomacy.c:1918
#, c-format
msgid ""
"*%s (AI)* Greetings friend, may we suggest making a common cause and join in "
"an alliance?"
msgstr ""
"*%s (AI)* Tervehdys, ystävä, haluaisimme ehdottaa yhteisen edun ajamista "
"liittoutumalla."

#: ai/default/daidiplomacy.c:1935
#, c-format
msgid "*%s (AI)* Greetings neighbor, may we suggest more peaceful relations?"
msgstr ""
"*%s (AI)* Tervehdys, naapuri, haluaisimme ehdottaa rauhanomaisempia suhteita."

#: ai/default/daidiplomacy.c:1952
#, c-format
msgid ""
"*%s (AI)* We grow weary of this constant bloodshed. May we suggest a "
"cessation of hostilities?"
msgstr ""
"*%s (AI)* Tämä jatkuva verenvuodatus väsyttää meitä. Voisimmeko haudata "
"sotakirveen?"

#: ai/default/daidiplomacy.c:2240
#, c-format
msgid ""
"*%s (AI)* We have been savagely attacked by %s, and we need your help! Honor "
"our glorious alliance and your name will never be forgotten!"
msgstr ""
"*%s (AI)* %s on hyökännyt julmasti kimppuumme ja tarvitsemme apuanne! "
"Kunnioittakaa mahtavaa liittoamme ja nimeänne kunnioitetaan iäti!"

#: ai/difficulty.c:252
msgid ""
"Toggles 'away' mode for your nation. In away mode, the AI will govern your "
"nation but make only minimal changes."
msgstr ""
"Vaihtaa pelaajan paikallaolotilaa. Pelaajan ollessa poissa tekoäly hallitsee "
"kansakuntaa mutta tekee vain minimaalisia muutoksia."

#: ai/difficulty.c:258
#, c-format
msgid ""
"With no arguments, sets all AI players to skill level '%s', and sets the "
"default level for any new AI players to '%s'. With an argument, sets the "
"skill level for the specified player only."
msgstr ""
"Ilman argumentteja komento asettaa kaikkien AI-pelaajien taidot tasolle '%s' "
"ja asettaa myös uusien AI-pelaajien tasoksi '%s'. Nimen kanssa vain kyseisen "
"AI-pelaajan vaikeustasoa asetetaan."

#: ai/difficulty.c:276
msgid "Has erratic decision-making."
msgstr "Tekee ailahtelevia päätöksiä."

#: ai/difficulty.c:283
#, c-format
msgid "Research takes %d%% as long as usual."
msgstr "Tutkimus kestää %d%% tavallisesta ajasta."

#: ai/difficulty.c:287
msgid "Has reduced appetite for expansion."
msgstr "Tavallista vähemmän kiinnostunut laajentumisesta."

#: ai/difficulty.c:294
msgid ""
"\n"
"This skill level has the same features as 'Novice', but may suffer "
"additional ruleset-defined penalties."
msgstr ""
"\n"
"Tällä taitotasolla on samat ominaisuudet kuin 'Vasta-alkajalla', mutta se "
"saattaa kärsiä sääntökokoelman määräämistä lisärangaistuksista."

#: ai/difficulty.c:300
msgid ""
"\n"
"This skill level has the same features as 'Hard', but may enjoy additional "
"ruleset-defined bonuses."
msgstr ""
"\n"
"Tällä taitotasolla on samat ominaisuudet kuin 'Vaikealla', mutta se saattaa "
"nauttoia sääntökokoelman antamista bonuksista."

#: ai/difficulty.c:309
msgid ""
"\n"
"This skill level's features include the following. (Some rulesets may define "
"extra level-specific behavior.)"
msgstr ""
"\n"
"Tämän taitotason ominaisuuksia ovat ainakin seuraavat. (Sääntökokoelma voi "
"määrittää lisää taitotasokohtaisia ominaisuuksia.)"

#: ai/handicaps.c:86
msgid "Doesn't build offensive diplomatic units."
msgstr "Ei rakenna hyökkääviä diplomaattiyksiköitä."

#: ai/handicaps.c:90
msgid "Gets reduced bonuses from huts."
msgstr "Saa majoista heikennettyjä bonuksia."

#: ai/handicaps.c:92
msgid "Prefers defensive buildings and avoids close diplomatic relations."
msgstr ""
"Keskittyy puolustusrakennuksien rakentamiseen ja välttää läheisiä "
"diplomaattisuhteita."

#: ai/handicaps.c:95
msgid ""
"THIS IS ONLY FOR TESTING OF NEW AI FEATURES! For ordinary servers, this "
"level is no different to 'Hard'."
msgstr ""
"TÄMÄ ON TARKOITETTU VAIN UUSIEN TEKOÄLYOMINAISUUKSIEN TESTAAMISEEN! "
"Normaaleilla palvelimilla tämä ei eroa millään tavalla 'Vaikeasta'."

#: ai/handicaps.c:99
msgid "Has no restrictions on tax rates."
msgstr "Ei rajoituksia veroasteiden asettamisessa."

#: ai/handicaps.c:102
msgid "Can target units and cities in unseen or unexplored territory."
msgstr ""
"Voi suunnitella hyökkäyksiä sellaisiakin yksiköitä ja kaupunkeja vastaan, "
"joita ei näe."

#: ai/handicaps.c:105
msgid "Knows the location of huts in unexplored territory."
msgstr "Tietää majojen sijainnin tutkimattomalla alueella."

#: ai/handicaps.c:108
msgid "Can see through fog of war."
msgstr "Näkee kaiken alueella, jonka on joskus tutkinut."

#: ai/handicaps.c:110
msgid "Doesn't build air units."
msgstr "Kaupungin perustaminen ei onnistu."

#: ai/handicaps.c:113
msgid "Has complete map knowledge, including unexplored territory."
msgstr "Omaa täydellisen karttatiedon, myös tutkimattomasta alueesta."

#: ai/handicaps.c:115
msgid "Naive at diplomacy."
msgstr "Yksinkertainen diplomatiassa."

#: ai/handicaps.c:118
msgid "Can skip anarchy during revolution."
msgstr "Välttää anarkiajakson hallitusmuodon vaihdossa."

#: ai/handicaps.c:120
msgid "Limits growth to match human players."
msgstr "Rajoittaa kasvunopeutensa korkeintaan ihmispelaajien tasoon."

#: ai/handicaps.c:122
msgid "Believes its cities are always under threat."
msgstr "Luulee kaupunkiensa olevan jatkuvasti vaarassa."

#: ai/handicaps.c:124
msgid "Always offers cease-fire on first contact."
msgstr "Tarjoaa aina tulitaukoa ensitapaamisella."

#: ai/handicaps.c:126
msgid "Doesn't bribe worker or city founder units."
msgstr "Ei lahjo työläis- tai kaupunginperustajayksiköitä."

#: ai/handicaps.c:129
msgid "Can change city production type without penalty."
msgstr "Voi vaihtaa kapungin tuotantotyyppiä ilman rangaistuksia."

#: ai/handicaps.c:131
msgid "Limits the distance to search for threatening enemy units."
msgstr ""
"Rajoittaa kuinka kaukana olevien vihollisyksiköiden liikkeitä ennakoidaan."

#: ai/tex/texai.c:594
msgid ""
"This Freeciv compilation has no full threads implementation, tex ai cannot "
"be used."
msgstr ""
"Tässä Freeciv-käännöksessä ei ole täyttä säikeiden tukea, säikeistettyä "
"tekoälymodulia (AI) 'tex' ei voida käyttää."

#: client/agents/cma_core.c:402 server/cityturn.c:422
#, c-format
msgid ""
"The citizen governor can't fulfill the requirements for %s. Passing back "
"control."
msgstr ""
"Käskynhaltija ei voi täyttää vaatimuksia kaupungissa %s. Palautan hallinnan."

#: client/agents/cma_core.c:411 client/agents/cma_core.c:431
#, c-format
msgid ""
"The citizen governor has gotten confused dealing with %s.  You may want to "
"have a look."
msgstr ""
"Käskynhaltija hämmentyi, kun käsittelyssä oli %s. Halunnet katsoa, mitä on "
"tekeillä."

#: client/agents/cma_fec.c:226 client/gui-gtk-3.22/cityrep.c:771
#: client/gui-gtk-3.22/cityrep.c:797 client/gui-gtk-4.0/cityrep.c:782
#: client/gui-gtk-4.0/cityrep.c:821 client/gui-gtk-5.0/cityrep.c:803
#: client/gui-gtk-5.0/cityrep.c:842
msgid "?cma:none"
msgstr "asettamatta"

#: client/agents/cma_fec.c:242 client/gui-gtk-3.22/cityrep.c:818
#: client/gui-gtk-4.0/cityrep.c:849 client/gui-gtk-5.0/cityrep.c:870
msgid "?cma:custom"
msgstr "oma valinta"

#. TRANS: city is not growing.  Keep short.
#. TRANS: city is not growing. Keep short.
#: client/agents/cma_fec.c:257 client/agents/cma_fec.c:291
#: client/citydlg_common.c:311 client/citydlg_common.c:369
#: client/citydlg_common.c:473 client/gui-gtk-3.22/citydlg.c:1972
#: client/gui-gtk-4.0/citydlg.c:1959 client/gui-gtk-5.0/citydlg.c:1959
