msgstr "Axudamarabilla"

#: data/civ1/units.ruleset:27 data/civ2/units.ruleset:32 data/classic/units.ruleset:39
#: data/sandbox/units.ruleset:45 data/civ2civ3/units.ruleset:41
#: data/multiplayer/units.ruleset:38
msgid "AddToCity"
msgstr "EngadirACidade"

#: data/civ1/units.ruleset:28 data/civ1/units.ruleset:1350 data/civ2/units.ruleset:33
#: data/civ2/units.ruleset:2143 data/classic/units.ruleset:40
