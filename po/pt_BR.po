"  1 = terrestre\n"
"  2 = marítimo\n"
"  4 = helitransportado\n"
"  8 = aéreo"

#: server/settings.c:682
msgid "National borders radius"
msgstr "Diminuir raio da sombra"

#: server/settings.c:683
msgid ""
