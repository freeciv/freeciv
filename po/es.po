msgstr "Dividido"

#. TRANS: enum value for 'gui_gtk2/gtk3_message_chat_location'
#: client/options.c:1684
msgid "Separate"
msgstr "Separado"

#. TRANS: enum value for 'gui_gtk2/gtk3_message_chat_location'
#: client/options.c:1686
msgid "Merged"
msgstr "Fusionado"

#. TRANS: enum value for 'gui_popup_tech_help'
#: client/options.c:1702 server/settings.c:289
msgid "Enabled"
msgstr "Habilitado"

#. TRANS: enum value for 'gui_popup_tech_help'
