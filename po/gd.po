#: server/civserver.c:235
#: client/client_main.c:251
#: manual/civmanual.c:380
#, c-format
msgid ""
"Usage: %s [option ...]\n"
"Valid options are:\n"
msgstr ""
"Cleachdadh: %s [roghainn ...]\n"
"Roghainnean dligheach:\n"

#: server/civserver.c:236
msgid "  -A  --Announce PROTO\tAnnounce game in LAN using protocol PROTO (IPv4/IPv6/none)\n"
msgstr "  -A  --Announce PROTOCAL\tNochd an geama anns an LAN tron phrotocal PROTO (IPv4/IPv6/none)\n"

#: server/civserver.c:238
msgid "  -a  --auth FILE\tEnable server authentication with configuration from FILE.\n"
msgstr "  -a  --auth FAIDHLE\tDèan dearbhadh frithealaiche comasach leis a' cho-chumadh o FAIDHLE.\n"

#: server/civserver.c:240
msgid "  -G  --Guests\t\tAllow guests to login if auth is enabled.\n"
msgstr "  -G  --AOIGHEAN\t\tThoir cead do dh'aoighean logadh a-steach ma bhios an dearbhadh comasach.\n"

#: server/civserver.c:242
msgid "  -N  --Newusers\tAllow new users to login if auth is enabled.\n"
msgstr "  -N  --Cleachdaicheanùra\tThoir cead do chleachdaichean ùra logadh a-steach ma bhios an dearbhadh comasach.\n"

#: server/civserver.c:245
msgid "  -b  --bind ADDR\tListen for clients on ADDR\n"
msgstr "  -b  --bind SEÒLADH\tCum cluas ri cliantan air SEÒLADH\n"

#: server/civserver.c:247
#: client/client_main.c:256
msgid "  -d, --debug NUM\tSet debug log level (0 to 4, or 4:file1,min,max:...)\n"
msgstr "  -d, --debug ÀIREAMH\tSuidhich leibheil loga dì-bhugadaireachd (0 gu 4, no 4:file1,min,max:...)\n"

#: server/civserver.c:251
#: client/client_main.c:260
msgid "  -d, --debug NUM\tSet debug log level (0 to 3)\n"
msgstr "  -d, --debug ÀIREAMH\tSuidhich leibheil loga dì-bhugadaireachd (0 gu 3)\n"

#: server/civserver.c:253
msgid "  -f, --file FILE\tLoad saved game FILE\n"
msgstr "  -f, --file FAIDHLE\tLuchdaich FAIDHLE de gheama a chaidh a shàbhaladh\n"

#: server/civserver.c:255
#: client/client_main.c:263
msgid "  -h, --help\t\tPrint a summary of the options\n"
msgstr "  -h, --help\t\tClò-bhuail gearr-chunntas nan roghainnean\n"

#: server/civserver.c:256
msgid "  -i, --identity ADDR\tBe known as ADDR at metaserver\n"
msgstr "  -i, --identity SEÒLADH\tBidh SEÒLADH air mar ainm aig an fhrithealaiche mheata\n"

#: server/civserver.c:257
msgid "  -l, --log FILE\tUse FILE as logfile\n"
msgstr "  -l, --log FAIDHLE\tCleachd FAIDHLE mar fhaidhle loga\n"

#: server/civserver.c:258
msgid "  -m, --meta\t\tNotify metaserver and send server's info\n"
msgstr "  -m, --meta\t\tLeig fios dhan fhrithealaiche mheata is cuir fiosrachadh an fhrithealaiche\n"

#: server/civserver.c:260
msgid "  -M, --Metaserver ADDR\tSet ADDR as metaserver address\n"
msgstr "  -M, --Metaserver SEÒLADH\tSuidhich SEÒLADH mar sheòladh an fhrithealaiche mheata\n"

#: server/civserver.c:263
msgid "  -p, --port PORT\tListen for clients on port PORT\n"
msgstr "  -p, --port PORT\tCum cluas ri cliantan air port PORT\n"

#: server/civserver.c:265
msgid "  -q, --quitidle TIME\tQuit if no players for TIME seconds\n"
msgstr "  -q, --quitidle TÌDE\tDùin e mura bi cluicheadairean ann as dèidh TÌDE diog\n"

#: server/civserver.c:267
msgid "  -e, --exit-on-end\tWhen a game ends, exit instead of restarting\n"
msgstr "  -e, --exit-on-end\tNuair a thig geama gu crìoch, dùin e is na ath-thòisich e\n"

#: server/civserver.c:270
msgid "  -s, --saves DIR\tSave games to directory DIR\n"
msgstr "  -s, --saves PASGAN\tSàbhail geamannan dhan phasgan PASGAN\n"

#: server/civserver.c:272
msgid "  -S, --Serverid ID\tSets the server id to ID\n"
msgstr "  -S, --Serverid DA\tSuidhich dearbh-aithne an fhrithealaiche mar DA\n"

#: server/civserver.c:274
msgid "  -P, --Ppm\t\tSave ppms of the map when saving the game.\n"
msgstr "  -P, --Ppm\t\tSàbhail ppms a' mhapa nuair a thèid an geama a shàbhaladh.\n"

#: server/civserver.c:275
msgid "  -r, --read FILE\tRead startup script FILE\n"
msgstr "  -r, --read FAIDHLE\tLeugh an sgriobt tòiseachaidh FAIDHLE\n"

#: server/civserver.c:277
msgid "  -R, --Ranklog FILE\tUse FILE as ranking logfile\n"
msgstr "  -R, --Ranklog FAIDHLE\tCleachd FAIDHLE mar fhaidhle loga rangachadh\n"

#: server/civserver.c:278
#: client/client_main.c:282
msgid "  -v, --version\t\tPrint the version number\n"
msgstr "  -v, --version\t\tClò-bhuail àireamh an tionndaidh\n"

