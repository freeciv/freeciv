msgid "waiting"
msgstr "a' fuireach"

#: client/plrdlg_common.c:209
#: client/gui-sdl/plrdlg.c:645
msgid "done"
msgstr "deiseil"

#: client/plrdlg_common.c:211
#: client/gui-sdl/plrdlg.c:647
msgid "moving"
msgstr "a' gluasad"

#: client/plrdlg_common.c:254
msgid "?Player:Name"
msgstr "?Player:Ainm"

#: client/plrdlg_common.c:255
msgid "Username"
msgstr "Ainm-cleachdaiche"

