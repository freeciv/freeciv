#: data/civ1/units.ruleset:512 data/civ2/units.ruleset:533
#: data/default/units.ruleset:533
msgid "Crusaders"
msgstr "Kruisvaarders"

#: data/civ1/units.ruleset:535 data/civ2/units.ruleset:556
#: data/default/units.ruleset:556
msgid "Knights"
msgstr "Ridders"

#: data/civ1/units.ruleset:559 data/civ2/units.ruleset:580
#: data/default/units.ruleset:580
msgid "Dragoons"
