#: data/default/units.ruleset:867 data/civ2/units.ruleset:804
#: data/civ1/units.ruleset:776 data/history/units.ruleset:867
msgid "Cannon"
msgstr "Kanon"

#: data/default/units.ruleset:895 data/civ2/units.ruleset:832
#: data/civ1/units.ruleset:832 data/history/units.ruleset:895
msgid "Artillery"
msgstr "Artilleri"

#: data/default/units.ruleset:923 data/civ2/units.ruleset:860
#: data/history/units.ruleset:923
msgid "Howitzer"
