#: data/default/units.ruleset:797 data/civ2/units.ruleset:739
#: data/civ1/units.ruleset:319
msgid "Cavalry"
msgstr "Kawaleria"

#: data/default/units.ruleset:825 data/civ2/units.ruleset:767
#: data/civ1/units.ruleset:404
msgid "Armor"
msgstr "Czołg"

#: data/default/units.ruleset:853 data/civ2/units.ruleset:795
#: data/civ1/units.ruleset:432
