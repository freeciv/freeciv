#: server/stdinhand.c:1418
msgid "Name is too long, so cannot be a connection."
msgstr "Navnet er for langt, så det kan ikke være en tilkobling."

#: server/stdinhand.c:1422
#, c-format
msgid "Connection name prefix '%s' is ambiguous."
msgstr "Spillernavn-prefikset '%s' er ikke-entydig."

#: server/stdinhand.c:1426
#, c-format
msgid "No connection by the name of '%s'."
msgstr "Ingen tilkobling med navn '%s'."

#: server/stdinhand.c:1445
#, c-format
msgid "Open metaserver connection to [%s]."
