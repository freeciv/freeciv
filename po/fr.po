#: server/stdinhand.c:1988
#, c-format
msgid "Bad rulesout type: '%s' (%s).  Try '%shelp rulesout'."
msgstr "Mauvais type de règles : '%s' (%s). Essayez '%shelp rulesout'."

#: server/stdinhand.c:1997
#, c-format
msgid "Saved techs rules data to '%s'."
msgstr "Données des règles 'techs' sauvées dans '%s'."

# AR
#: server/stdinhand.c:2000
#, c-format
msgid "Failed saving techs rules data to '%s'."
msgstr "Échec dans la sauvegarde des données des règles 'techs' dans '%s'."

