[binaries]
cc = '<EMSDK_ROOT>/upstream/emscripten/emcc'
cpp = '<EMSDK_ROOT>/upstream/emscripten/em++'
ar = '<EMSDK_ROOT>/upstream/emscripten/emar'
strip = '<EMSDK_ROOT>/upstream/emscripten/emstrip'

[properties]
cross_lib_path = '<EMSDK_ROOT>/upstream/emscripten/system/lib'
crosser = false

[host_machine]
system = 'emscripten'
cpu_family = 'wasm32'
cpu = 'wasm32'
endian = 'little'
